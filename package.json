{
  "name": "pulse-networks",
  "version": "0.1.1",
  "description": "Website for Pulse Networks Limited",
  "repository": "https://github.com/JessicaNeary/pulse-networks.git",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --content-base public/ --hot --inline",
    "watch": "nodemon --watch server server/index & webpack --watch",
    "test": "tape -r babel-register 'test/**/*.test.js' | tap-spec",
    "start": "node server/index",
    "build-css": "node-sass -w public/css/style.scss public/css/main.css",
    "heroku-postbuild": "webpack -p --config ./webpack.config.js --progress"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Jessica Neary",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jsx-loader": "^0.13.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router": "^3.0.1",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "css-loader": "^2.1.0",
    "enzyme": "^2.7.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "sass": "^0.5.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "webpack-dev-server": "^1.16.2"
  }
}
